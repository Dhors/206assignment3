<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ListController">
   <children>
      <Text layoutX="225.0" layoutY="325.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Current creations" />
      <ListView fx:id="listViewCreations" layoutX="138.0" layoutY="113.0" onMouseClicked="#handleSelectedCreation" prefHeight="200.0" prefWidth="298.0" />
      <Button fx:id="playButton" layoutX="225.0" layoutY="329.0" mnemonicParsing="false" onAction="#handlePlayButton" text="Play" />
      <Button fx:id="deleteButton" layoutX="282.0" layoutY="329.0" mnemonicParsing="false" onAction="#handleDeleteButton" text="Delete" />
      <Button layoutX="368.0" layoutY="84.0" mnemonicParsing="false" onAction="#handleRefreshButton" text="Refresh" />
      <Text fx:id="selectPrompt" layoutX="185.0" layoutY="375.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please select a creation to continue." />
      <Button fx:id="newCreationButton" layoutX="138.0" layoutY="84.0" mnemonicParsing="false" onAction="#handleNewCreationButton" text="New Creation" />
      <Text layoutX="271.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Varpedia" />
   </children>
</AnchorPane>
