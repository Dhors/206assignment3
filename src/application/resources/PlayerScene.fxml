<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: a9e5ff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.playerController">
   <children>
      <Text fx:id="_videoTitle" layoutX="255.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player" />
      <Button fx:id="_returnButton" layoutX="70.0" layoutY="477.0" mnemonicParsing="false" onAction="#handleReturnButton" text="Return" />
      <Button layoutX="311.0" layoutY="438.0" mnemonicParsing="false" onAction="#handlePausePlayButton" text="pause/play" />
      <Button layoutX="401.0" layoutY="438.0" mnemonicParsing="false" onAction="#handleForwardButton" text="&gt;&gt;" />
      <Button layoutX="271.0" layoutY="438.0" mnemonicParsing="false" onAction="#handleBackwardsButton" text="&lt;&lt;" />
      <Button layoutX="210.0" layoutY="438.0" mnemonicParsing="false" onAction="#handleMuteButton" text="mute" />
      <BorderPane fx:id="_player" layoutX="65.0" layoutY="14.0" prefHeight="400.0" prefWidth="500.0">
         <center>
            <MediaView fx:id="_mediaView" fitHeight="300.0" fitWidth="500.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
