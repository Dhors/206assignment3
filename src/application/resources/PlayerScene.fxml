<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.playerController">
   <children>
      <Text fx:id="_videoTitle" layoutX="280.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player" />
      <Button fx:id="_returnButton" layoutX="14.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleReturnButton" text="Return" />
      <Pane fx:id="_player" layoutX="152.0" layoutY="53.0" prefHeight="250.0" prefWidth="296.0" />
      <Button layoutX="255.0" layoutY="321.0" mnemonicParsing="false" onAction="#handlePausePlayButton" text="pause/play" />
      <Button layoutX="345.0" layoutY="321.0" mnemonicParsing="false" onAction="#handleForwardButton" text="&gt;&gt;" />
      <Button layoutX="215.0" layoutY="321.0" mnemonicParsing="false" onAction="#handleBackwardsButton" text="&lt;&lt;" />
      <Button layoutX="154.0" layoutY="321.0" mnemonicParsing="false" onAction="#handleMuteButton" text="mute" />
   </children>
</AnchorPane>
